{% extends 'base.html.twig' %}

{% block title %}SousMetier index{% endblock %}

{% block body %}
   
   <h1 style="text-align: center;"><span style="color: #0077B6;">Sous Metier </span></h1>

    <table border="2" class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Libelle</th>
                <th>Domaine</th>
                
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for sous_metier in sous_metiers %}
            <tr>
                <td>{{ sous_metier.id }}</td>
                <td>{{ sous_metier.libelle }}</td>
                <td>{{ sous_metier.domaine }}</td>
                
                <td>
                    <a href="{{ path('app_sous_metier_show', {'id': sous_metier.id}) }}">show</a>
                    <a href="{{ path('app_sous_metier_edit', {'id': sous_metier.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="5">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_sous_metier_new') }}">Ajouter Sous_Metier</a>



    
{% endblock %}
